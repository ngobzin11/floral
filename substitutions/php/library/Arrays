__ARRAYS_FILL_FROM__						array_fill(%3$S, %1$S, %2$S)
__ARRAYS_CONTAINS__							in_array(%2$s, %1$s)
__ARRAYS_MERGE__							array_merge(%s, %s)
__ARRAYS_REVERSE_SORT__						arsort(%s)
__ARRAYS_RANGE_TO_BY__						range(%s, %s, %s)
__ARRAYS_RANGE_TO__							range(%s, %s)
__ARRAYS_SUM__								array_sum(%s)
__ARRAYS_REVERSE__							array_reverse(%s)
__ARRAYS_PAD__								array_pad(%s, %s, %s)