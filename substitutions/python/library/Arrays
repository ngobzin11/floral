__ARRAYS_REMOVE_AT__						%2$s.pop(%1$s)
__ARRAYS_CONTAINS__							%2$s in %1$s
__ARRAYS_MERGE__							%s + %s
__ARRAYS_REVERSE_SORT__						%s.sort(reverse=True)
__ARRAYS_RANGE_TO_BY__						range(%s, %s, %s)
__ARRAYS_RANGE_TO__							range(%s, %s)
__ARRAYS_SUM__								sum(%s)
__ARRAYS_REVERSE__							%s.reverse()
__ARRAYS_PAD__								%1$s = [%3$s] * (%2$s - len(%1$s))